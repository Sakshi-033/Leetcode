bool carPooling(int** trips, int tripsSize, int* tripsColSize, int capacity) {
     int veh[1001]={0};
    for(int i = 0;i<tripsSize;i++){
        int pg = trips[i][0];
        int start = trips[i][1];
        int end = trips[i][2];
        veh[start]+= pg;
        veh[end]-=pg;
    }
    for(int i = 1;i<1001;i++){
        veh[i]=veh[i]+veh[i-1]; }
    for(int i =0;i<1001;i++){
        if(veh[i]>capacity){
            return false;}
        
    }
    return true;

           
    
}
